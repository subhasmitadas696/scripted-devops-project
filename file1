pipeline{
    agent any
    stages("different statge "){
    
    stage("connecting with GITHUB"){
    steps{
    git branch: 'main', url: 'https://github.com/zafar-khan123/declarative-pipeline.git'
    }
    
    }
        stage("Build"){
            steps{
                echo "Building code using jenkins"
            }
            
        }
        
        stage("Testing"){
            steps{
                echo "Testing code"
            }
            
        }
        
        stage("Deploy"){
            steps{
                echo "Deploying code"
            }
        }
    }
}
